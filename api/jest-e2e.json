{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testMatch": ["<rootDir>/test/**/*.e2e-spec.{ts,js}"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverage": true,
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.(t|j)s"
  ],
  "coverageDirectory": "./coverage"
}
