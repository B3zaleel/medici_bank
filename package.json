{
  "name": "medici_bank",
  "version": "0.0.1",
  "description": "A simple banking application with NestJS, GraphQL, and MongoDB",
  "author": "Bezaleel Olakunori <bezaleeloci@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest-e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.11",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/platform-express": "^8.0.0",
    "apollo-server-express": "^3.7.0",
    "argon2-ffi": "^2.0.0",
    "dotenv": "^16.0.1",
    "graphql": "^16.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-morph": "^14.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/B3zaleel/medici_bank/-/tree/main/"
  },
  "bugs": {
    "url": "https://gitlab.com/B3zaleel/medici_bank/-/issues"
  },
  "homepage": "https://gitlab.com/B3zaleel/medici_bank/-/blob/main/README.md",
  "keywords": [
    "bank",
    "graphql",
    "nest.js",
    "mongodb",
    "vue",
    "typescript",
    "jest",
    "api"
  ],
  "engines": {
    "node": "14.x",
    "npm": "6.x",
    "yarn": "1.x"
  }
}
